    "msgpack-lite": "^0.1.26",
    "notepack": "0.0.2",
    "open": "^8.2.1",
    "pacote": "^11.3.5",
    "tar": "^6.1.8",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2",
    "wabt": "^1.0.24",
    "websocket": "^1.0.34",
    "wtf-8": "^1.0.1",
    "es-arraybuffer-base64": "^1.1.2"
