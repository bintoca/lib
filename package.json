  },
  "homepage": "https://github.com/bintoca/lib#readme",
  "devDependencies": {
    "@types/cacache": "^15.0.1",
    "@types/jest": "^29.5.13",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.1",
    "@types/tar": "^4.0.5",
    "acorn": "^8.4.1",
    "acorn-globals": "^6.0.0",
    "acorn-walk": "^8.1.1",
    "anymatch": "^3.1.2",
    "async": "^3.2.1",
    "benchmark": "^2.1.4",
    "borc": "^3.0.0",
    "cacache": "^15.3.0",
    "cachedir": "^2.3.0",
