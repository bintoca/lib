{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "lib": [
            "es2020",
            "dom",
            "WebWorker"
        ],
        "module": "es2020",
        "target": "es2019",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "preserveConstEnums": true
    }
}